<view><view class="cu-list menu-avatar"><view class="cu-item"><view class="cu-avatar round lg bg-blue"><text class="cuIcon-new text-white"></text></view><view data-event-opts="{{[['tap',[['toInbox',['$event']]]]]}}" class="content" bindtap="__e"><view class="text-df">私信</view><view class="text-gray text-sm flex"><view class="text-cut">{{''+(userInfo.user_info.inbox_unread?'你有'+userInfo.user_info.inbox_unread+'条新私信':'暂无新私信')+''}}</view></view></view><block wx:if="{{userInfo.user_info&&userInfo.user_info.inbox_unread>0}}"><view class="action"><view class="cu-tag round bg-blue sm">{{userInfo.user_info.inbox_unread?userInfo.user_info.inbox_unread:0}}</view></view></block></view></view><view class="cu-bar bg-white solid-bottom margin-top"><view class="action"><text class="cuIcon-notice text-blue "></text>通知列表</view><view class="action"><button data-event-opts="{{[['tap',[['toRead',[-1,true]]]]]}}" class="cu-btn bg-blue shadow sm" bindtap="__e">全部已读</button></view></view><view class="cu-list"><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toRead',[index,false]]]]]}}" class="flex flex-wrap bg-white padding" bindtap="__e"><view class="notiy_left"><text>{{item.message}}</text></view><view class="notiy_right text-right"><text>{{item.add_time}}</text><text class="{{['padding-left-xs cuIcon-title',item.read_flag==0?'text-blue light':'text-gary']}}"></text></view></view></block></view><block wx:if="{{listData.length<1}}"><view class="bg-white text-center padding">暂无未读通知信息</view></block><block wx:if="{{system_config.adunitid&&system_config.adunitid!=''}}"><view class="margin-tb-sm"><ad unit-id="{{system_config.adunitid}}"></ad></view></block><view class="cu-tabbar-height"></view></view>