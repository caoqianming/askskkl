<view><block wx:if="{{listData.length>1}}"><view class="cu-bar bg-white solid-bottom"><view class="action"><text class="cuIcon-new text-blue "></text>私信列表</view></view></block><view class="cu-list menu-avatar"><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toChat',['$0'],[[['listData','',index,'id']]]]]]]}}" class="cu-item" bindtap="__e"><view class="cu-avatar round lg" style="{{('background-image:url('+item.avatar_file+');')}}"></view><view class="content"><view class="text-black">{{item.user_name}}</view><view class="text-gray text-sm flex"><view class="text-cut"><block wx:if="{{item.unread>0}}"><text class="text-blue margin-right-xs">[未读]</text></block>{{item.last_message+''}}</view></view></view><view class="action"><view class="text-grey text-xs">{{item.update_time}}</view><block wx:if="{{item.unread>0}}"><view class="cu-tag round bg-blue sm">{{item.count}}</view></block></view></view></block></view><block wx:if="{{listData.length<1}}"><page-tips vue-id="1" tipsText="暂无私信" iconClass="cuIcon-new text-gray" bind:__l="__l"></page-tips></block><block wx:if="{{system_config.adunitid&&system_config.adunitid!=''}}"><view class="margin-tb-sm"><ad unit-id="{{system_config.adunitid}}"></ad></view></block><view class="cu-tabbar-height"></view></view>